# Copy this file to .env and fill in real values (never commit real secrets)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=public_anon_key
# Frontend should NEVER receive the service key. Use only in server scripts (do not prefix with VITE_).
SUPABASE_SERVICE_KEY=service_role_key_for_node_scripts_only

# Gemini / Generative AI (optional)
VITE_GEMINI_KEY=your_gemini_api_key

# Other optional flags
VITE_APP_ENV=development
